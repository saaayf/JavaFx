         <font>
            <Font name="System Bold" size="27.0" />
         </font>
      </Label>
      <Button fx:id="BtnAjouter" layoutX="615.0" layoutY="525.0" mnemonicParsing="false" onAction="#Ajouter" prefHeight="49.0" prefWidth="101.0" text="Ajouter" />
      <TextField fx:id="SearchField" layoutX="69.0" layoutY="78.0" prefHeight="57.0" prefWidth="213.0" promptText="Recherche">
         <font>
            <Font size="24.0" />
         </font>
      </TextField>
      <ComboBox fx:id="ComboBudget" layoutX="556.0" layoutY="77.0" prefHeight="58.0" prefWidth="159.0" promptText="Filtrer par budget" />
   </children>
</AnchorPane>
